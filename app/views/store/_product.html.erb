      <%= image_tag product.image_url %>
      <h2><%= product.title%></h2>
      <p>
        <%= sanitize product.description %>
      </p>
      <div class="price">
        <%=number_to_currency product.price%>
      </div>
      <%= button_to "Add To Cart", line_items_path(product_id: product.id) %>